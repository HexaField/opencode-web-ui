{
  "name": "solid-tailwind-ts-vite-template",
  "version": "1.1.0",
  "author": "AR10",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite dev",
    "dev:server": "tsx watch server/src/index.ts",
    "start:server": "tsx server/src/index.ts",
    "start:test": "concurrently \"npm run start:server\" \"npm run dev:client\"",
    "test:e2e": "playwright test",
    "build": "vite build",
    "preview": "vite preview",
    "prettier": "prettier . -c",
    "prettier:fix": "prettier . -w",
    "lint": "tsc --noEmit && eslint .",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@ianvs/prettier-plugin-sort-imports": "4.7.0",
    "@playwright/test": "^1.57.0",
    "@solidjs/testing-library": "^0.8.10",
    "@tailwindcss/vite": "4.1.18",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/supertest": "^6.0.3",
    "concurrently": "^9.2.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "jsdom": "^27.3.0",
    "lightningcss": "1.30.2",
    "prettier": "3.7.4",
    "prettier-eslint-cli": "8.0.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "supertest": "^7.1.4",
    "tailwindcss": "4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "8.50.0",
    "vite": "7.3.0",
    "vite-plugin-solid": "2.11.10",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@opencode-ai/sdk": "^1.0.166",
    "body-parser": "^2.2.1",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "solid-js": "1.9.10"
  }
